<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chenjin.dao.BorrowHistoryDAO">
	<insert id="insertOne">
		insert into
		borrow_history(id,bid,bname,u_account,borrow_time,return_time)
		values(#{id},#{bid},#{bname},#{u_account},#{borrow_time},#{return_time})
	</insert>
</mapper>