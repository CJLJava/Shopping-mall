<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chenjin.dao.UserDAO">
	<insert id="insertOne">
		INSERT INTO `shopping`.`user_main_message` (
		`userName`, `password`,
		`province`, `city`, `county`,
		`detailedAddress`, `phone`) VALUES (
		#{userName}, #{password},
		#{province}, #{city}, #{county}, #{detailedAddress}, #{phone});
	</insert>
</mapper>